<% if @url_file_list.presence %>
    <% title = "ファイルリスト"%>
<% else %>
    <% if params[:dir] == ArtistsController::DIR_TYPE::SMARTPHONE %>
        <% title = %!「SP」 url 一覧  #{params[:title]}/dir:#{params[:dir]}! %>
    <% else %>
        <% title = %!「#{params[:filename]}」 url 一覧  #{params[:title]}/dir:#{params[:dir]}! %>
    <% end %>
<% end %>

<% provide(:title, title) %>

<h1><%= title %></h1>

<% if params[:show_times].presence %>
    <% show_times = params[:show_times].to_i %>
    <% if show_times > 1 %>
        <br />
        ・<%= show_times %>回以上のURLのみ
    <% end %>
<% end %>

<% if @pred %>
    <br />
    ・予測<%= @pred %>以上
<% end %>

<% if @hide_day %>
    <br />
    ・<%= @hide_day %>日以上経過
<% end %>


<% thumb_disp = false %>

<% pic_mag_pxv = '28%' %>
<% pic_mag_twt = '23%' %>
<% pic_mag_twt_a = '5%' %>
<% pic_mag_twt_b = '10%' %>

<% twt_disp_number = 10 %>
<% pxv_disp_number = 5 %>
<% if params[:target] == ArtistsController::FileTarget::TWT_UNKNOWN_ONLY %>
    <% pxv_disp_number = 6 %>
<% end %>


<!-- twt -->
<% twt_group = Hash.new { |h, k| h[k] = [] } %>

<% if params[:target] == ArtistsController::FileTarget::TWT_EXPERIMENT %>
    <% @known_twt_url_list.each do |key, url_list| %>
        <% twt_account = Twitter.find_by_twtid_ignore_case(key) %>
        <% if !twt_account.last_access_datetime_p(@hide_day) %>
            <% twt_group[url_list.size] << [twt_account, url_list] %>
        <% end %>
    <% end %>

    <% @known_twt_url_list = nil %>
<% end %>

<% if @known_twt_url_list.presence %>
    <% @known_twt_url_list.each do |key, url_list| %>
        <% if show_times.presence and url_list.size < show_times %>
            <!-- 飛ばす -->
            <% next %>
        <% end %>

        <% twt_account = Twitter.find_by_twtid_ignore_case(key) %>

        <% if @pred %>
            <% pred = @pred %>
        <% else %>
            <% pred = 0 %>
        <% end %>
        <% unless twt_account.select_cond_aio(pred) %>
            <%# next %>
        <% end %>

        <% key_twt = twt_account.group_key() %>
        <% twt_group[key_twt] << [twt_account, url_list] %>
    <% end %>
<% end %>

<% if @known_twt_url_list.presence %>
    <% known_twt_url_list_size = @known_twt_url_list.size %>
<% else %>
    <% known_twt_url_list_size = 0 %>
<% end %>

<% if known_twt_url_list_size > 0 %>
    <h2>登録済みtwt (<%= known_twt_url_list_size %>)</h2>
<% end %>

<%# filesize_list = Twt::get_pic_filesize_list %>
<%# STDERR.puts %!filesize_list=#{filesize_list.size}! %>

<% if twt_group.presence %>
    <% h_grp1_bak = "" %>
    <% twt_group = twt_group.sort.reverse.to_h #twt_group = twt_group.sort.to_h %>
    <% twt_group.each do |key, val| %>
        <% list = val %>
        <% unless list.presence %>
            <% next %>
        <% end %>

        <% h_ary = key.split(Twitter::TWT_H_SEPARATOR) if key.instance_of?(String) %>
        <% if h_ary.size == 1 %>
            <% h_grp2 = h_ary[0] %>
            <h3><%= h_grp2 %></h3>
        <% elsif h_ary.size == 2 %>
            <% h_grp1, h_grp2 = h_ary %>
        <% elsif h_ary.size == 3 %>
            <% h_grp1 = h_ary[0..1].join(Twitter::TWT_H_SEPARATOR) %>
            <% h_grp2 = h_ary[2] %>
        <% elsif h_ary.size >= 4 %>
            <% h_grp1 = h_ary[0..1].join(Twitter::TWT_H_SEPARATOR) %>
            <% h_grp2 = h_ary[2..-1].join(Twitter::TWT_H_SEPARATOR) %>
        <% else %>
            <% h_grp2 = key %>
        <% end %>

        <% if h_grp1 != h_grp1_bak %>
            <h2><%= h_grp1 %></h2>
            <% h_grp1_bak = h_grp1 %>
        <% end %>

        <%# puts %!"#{key}"! %>
        <h3><%= h_grp2 %>(<%= list.size %>)</h3>
        <table border="1" rules="all">
            <tr>
                <th>#</th>
                <th>method</th>
                <th>rating</th>
                <th>st</th>
                <th>name</th>
                <th>予測</th>
                <th>-</th>
                <th>access</th>
                <th>UL</th>
                <th>サムネイル</th>
                <th>pxv id</th>
                <th>name</th>
                <th>pred</th>
                <th>■</th>
                <th>access</th>
                <th>UL</th>
                <th>thumbnail</th>
                <th>X</th>
            </tr>
            <% list = list.sort_by {|z|
                x = z[0];
                x.sort_cond(z[1].size)
                }.reverse
            %>
            <!-- ,  -->
            <% list.first(twt_disp_number).each.with_index(1) do |val, idx| %>
                <% twt_account = val[0] %>
                <% url_list = val[1] %>
                <tr>
                    <td><%= idx %></td>
                    <td>
                        <%= twt_account.drawing_method %>
                    </td>
                    <td>
                        [<%= url_list.size %>]
                        【<%= twt_account.rating %>】<%= twt_account.r18 %>
                        <% if twt_account.newcomer? %>
                            <b>!NEW!</b>
                        <% end %>
                    </td>
                    <td>
                        <% if twt_account.status == "TWT巡回" %>
                            <%#= twt_account.status %>
                        <% else %>
                            <b><%= twt_account.status %></b>
                        <% end %>
                    </td>
                    <td>
                        <b>
                            <%= twt_account.twtname %>
                        </b>
                        (@<%= twt_account.twtid %>)
                        [<%= twt_account.update_frequency %>]
                    </td>
                    <% pred = twt_account.prediction %>
                    <% bgcolor = pred_bg_color(pred) #pre_bgcolor_ex(pred) %>
                    <td align="right" bgcolor="<%= bgcolor %>">
                        <%= pred %>
                    </td>
                    <td>
                        <% if twt_account.filesize %>
                            <% if twt_account.sp? #filesize_huge? %>
                                <font color="red">
                                    <%= sprintf("[%4d KB]", twt_account.filesize / 1024) %>
                                </font>
                            <% end %>
                        <% else %>
                            (未登録)
                        <% end %>
                        <%= link_to_ex "■twt■", twt_account %>
                    </td>
                    <td>
                        <%= twt_account.last_access_datetime_disp %>
                    </td>
                    <td>
                        <%= Util.get_date_info(twt_account.last_post_datetime) %>
                    </td>
                    <td>
                        <% if (thumb_disp and pred >= @pred) or (twt_account.rating == 0) %>
                            <% tlist = twt_account.get_pic_filelist_ex %>
                            <% tlist.first(3).each do |filepath| %>
                                <%= link_to_ex(image_tag(filepath, width: pic_mag_twt_b, height: pic_mag_twt_b), filepath) if filepath.presence %>
                            <% end %>
                        <% end %>
                        <%= twt_account.filenum %>
                        <%= %!(#{twt_account.update_frequency})! if false %>
                    </td>
                    <!-- pxv -->
                    <td>
                        <%= twt_account.pxvid %>
                    </td>
                    <% if twt_account.pxvid.presence %>
                        <% p = Artist.find_by(pxvid: twt_account.pxvid) %>
                    <% end %>
                    <% if p %>
                        <% p_pred = p.prediction_up_cnt(true) %>
                        <td>
                            【<%= p.rating %>|<%= p.feature %>】<%= p.pxvname %>
                        </td>
                        <td align="right">
                            <%= p_pred %>
                        </td>
                        <td>
                            <% if p.status %>
                                <%= p.status_disp(false) %>
                            <% end %>
                            <%= link_to_ex("■pxv■", artist_path(p, access_dt_update: "yes")) %>
                        </td>
                        <td>
                            <%= p.get_date_info(p.last_access_datetime) %>
                        </td>
                        <td>
                            <%= p.get_date_info(p.last_ul_datetime) %>
                        </td>
                        <td>
                            <% if thumb_disp and p_pred > @pred %>
                                <%= pic_path_tag(p.pxvid, 3, pic_mag_pxv) %>
                            <% end %>
                            <%= p.filenum %>
                        </td>
                    <% else %>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                    <% end %>
                    <% p = Artist.find_by_twtid_ignore_case(twt_account.twtid, false) %>
                    <% if p %>
                        <td>
                            <%= link_to_ex("■pxv■", artist_path(p, access_dt_update: "yes")) %>
                        </td>
                    <% else %>
                        <td>
                        </td>
                    <% end %>
                </tr>
            <% end %>
        </table>
        <% if list.size > twt_disp_number  %>
            残り：<%= list.size - twt_disp_number %>
        <% end %>
    <% end %>
<% end %>

